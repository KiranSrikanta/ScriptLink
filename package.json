{
  "name": "sharepoint-csr",
  "version": "1.0.0",
  "description": "Sample code showing the best practices for SharePoint CSR",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel build:dev:watch cdn test:watch",
    "build:dev": "SET NODE_ENV=development&& webpack",
    "build:dev:watch": "SET NODE_ENV=development&& webpack --watch",
    "build:prod": "SET NODE_ENV=production&& webpack",
    "cdn": "http-server dist/CDN -p 7777 -c-1",
    "lint": "eslint src/**/*.js",
    "test": "karma start",
    "test:watch": "karma start --auto-watch --no-single-run",
    "coverage": "SET NODE_ENV=coverage&&karma start"
  },
  "keywords": [
    "SharePoint",
    "LiveReload",
    "CSR",
    "UserCustomAction",
    "PnP"
  ],
  "author": "Kiran",
  "license": "MIT",
  "devDependencies": {
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "chai-jquery": "^2.0.0",
    "compression-webpack-plugin": "^0.3.1",
    "eslint": "^3.4.0",
    "eslint-watch": "^2.1.14",
    "fs": "0.0.1-security",
    "http-server": "^0.9.0",
    "isparta-loader": "^2.0.0",
    "jquery": "^3.1.0",
    "karma": "^1.2.0",
    "karma-chai-plugins": "^0.7.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.1.1",
    "karma-mocha-reporter": "^2.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "mocha": "^3.0.2",
    "npm-run-all": "^3.0.0",
    "webpack": "^1.13.2",
    "webpack-livereload-plugin": "^0.8.2"
  }
}
